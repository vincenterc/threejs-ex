{
  "name": "threejs-ex",
  "description": "Some practices of three.js with Gatsby",
  "version": "0.1.0",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.0.76",
    "gatsby-plugin-styled-components": "^3.0.4",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "styled-components": "^4.1.3",
    "three": "^0.100.0"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
